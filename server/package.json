{
  "name": "Biotops-server",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "scripts": {
    "start": "npm run copy-resources && npm run tsc && concurrently --kill-others \"node $NODE_DEBUG_OPTION dist/index.js\" \"npm run watch-copy-resources\" \"npm run watch-copy-templates\" \"npm run tsc-watch\"",
    "copy-resources": "cpx \"resources/**/*\" dist --include-empty-dirs & cpx \"src/**/*.hbs\" dist --include-empty-dirs & cpx \"src/**/*.sql\" dist",
    "watch-copy-resources": "cpx \"resources/**/*\" dist --watch --include-empty-dirs",
    "watch-copy-templates": "cpx \"src/**/*.{hbs}\" dist --watch --include-empty-dirs",
    "tsc": "tsc",
    "tsc-watch": "tsc -w",
    "build": "npm run copy-resources && npm run tsc && node compress_build.js"
  },
  "license": "MIT",
  "author": {
    "name": "Arturs Cvetkovs",
    "email": "arturs.cvetkovs@alpinweiss.eu"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:alpinweiss/Biotops.git"
  },
  "dependencies": {
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "express-handlebars": "^3.0.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "method-override": "^2.3.9",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "passport-facebook": "^2.1.1",
    "passport-jwt": "^3.0.0",
    "serve-favicon": "^2.4.3",
    "session-file-store": "^1.1.2",
    "winston": "^2.3.1",
    "mysql": "^2.14.1",
    "nconf": "^0.8.4",
    "node-schedule": "^1.2.3",
    "validator": "^8.0.0",
    "handlebars-helpers": "^0.9.6",
    "jsonwebtoken": "^7.4.2",
    "multer": "^1.3.0",
    "randomstring": "^1.1.5",
    "moment": "^2.18.1",
    "jwt-simple": "^0.5.1",
    "geolib": "^2.0.23",
    "exceljs": "^0.5.1",
    "facebook-graph": "^0.0.6"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "concurrently": "^3.5.0",
    "typescript": "^2.4.2",
    "archiver": "^2.0.3",
    "@types/express": "^4.0.36",
    "@types/express-handlebars": "^0.0.29",
    "@types/express-session": "^1.15.2",
    "@types/body-parser": "^1.16.4",
    "@types/cookie-parser": "^1.4.0",
    "@types/method-override": "^0.0.29",
    "@types/passport": "^0.3.3",
    "@types/passport-local": "^1.0.30",
    "@types/passport-strategy": "^0.2.30",
    "@types/passport-facebook": "^2.1.5",
    "@types/serve-favicon": "^2.2.28",
    "@types/winston": "^2.3.4",
    "@types/mysql": "^0.0.34",
    "@types/nconf": "^0.0.34",
    "@types/node-schedule": "^1.2.0",
    "@types/validator": "^6.2.2",
    "@types/multer": "^1.3.2",
    "@types/randomstring": "^1.1.6",
    "@types/jwt-simple": "^0.5.33",
    "@types/geolib": "^2.0.2",
    "@types/exceljs": "^0.5.2"
  }
}
